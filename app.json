{
    "expo": {
        "name": "lazermovies",
        "slug": "lazer-movies",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#0f0f0f"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "ilyasmirnov.reactlazermoviesapp",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#0f0f0f"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
